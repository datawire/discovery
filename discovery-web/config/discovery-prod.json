{
  "host": "0.0.0.0",
  "port": 52689,

  "authHandler": {
    "type": "jwt",
    "keyStore": {
      "type": "jceks",
      "path": "${DISCOVERY_KEYSTORE}",
      "password": "${DISCOVERY_KEYSTORE_PASSWORD}"
    }
  },

  "corsHandler": {
    "allowedOrigin": "*",
    "allowAuth": true,
    "allowMethods": [],
    "allowHeaders": [],
    "path": "/*"
  },

  "mixpanelIntegration": {
    "enabled": true,
    "token": "${MIXPANEL_TOKEN}",
    "publishFrequency": 120
  }
}